{
  "rules": {
    "presence": {
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    },
    "typing": {
      "$conversationId": {
        ".read": true,
        "$uid": {
          ".write": "$uid === auth.uid",
          ".validate": "newData.isNumber()"
        }
      }
    }
  }
}

